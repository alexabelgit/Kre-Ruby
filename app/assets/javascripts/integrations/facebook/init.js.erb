jQuery(document).on('turbolinks:load', function() {
    window.fbAsyncInit = function () {
        FB.init({
            appId:  '<%= ENV['FACEBOOK_APP_ID'] %>',
            status: true,
            cookie: true,
            xfbml:  true,
            oauth:  true
        });
        FB.Canvas.setAutoGrow();
    }
});